Module Module1
    Public Class Multiply ' 两数之积类
        Public Function Calculate(ByVal x As Double, ByVal y As Double) As Double
            Return x * y ' 计算两数之积
        End Function
    End Class
    Public Class NewAdd ' 两数之和类
        Public Function Calculate(ByVal x As Double, ByVal y As Double) As Double
            Return x + y ' 计算两数之和
        End Function
    End Class
End Module
